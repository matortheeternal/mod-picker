<div class="scaffold">
  <p id="notice"><%= notice %></p>

  <h1>lover_infos</h1>

  <table>
    <thead>
      <tr>
          <th>id</th>
          <th>mod_id</th>
          <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @lover_infos.each do |lover_info| %>
        <tr>
            <td><%= lover_info.id %></td>
            <td><%= lover_info.mod_id %></td>
            <td><%= link_to 'Show', lover_info %></td>
          <td><%= link_to 'Edit', edit_lover_info_path(lover_info) %></td>
          <td><%= link_to 'Destroy', lover_info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to 'New Lover Info', new_lover_info_path %>
</div>