<div class="scaffold">
  <p id="notice"><%= notice %></p>

  <h1>mod_list_compatibility_notes</h1>

  <table>
    <thead>
      <tr>
          <th>mod_list_id</th>
          <th>compatibility_note_id</th>
          <th>status</th>
          <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @mod_list_compatibility_notes.each do |mod_list_compatibility_note| %>
        <tr>
            <td><%= mod_list_compatibility_note.mod_list_id %></td>
            <td><%= mod_list_compatibility_note.compatibility_note_id %></td>
            <td><%= mod_list_compatibility_note.status %></td>
            <td><%= link_to 'Show', mod_list_compatibility_note %></td>
          <td><%= link_to 'Edit', edit_mod_list_compatibility_note_path(mod_list_compatibility_note) %></td>
          <td><%= link_to 'Destroy', mod_list_compatibility_note, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to 'New Mod List Compatibility Note', new_mod_list_compatibility_note_path %>
</div>