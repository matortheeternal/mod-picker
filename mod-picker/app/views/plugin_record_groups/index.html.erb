<p id="notice"><%= notice %></p>

<h1>Listing Plugin Record Groups</h1>

<table>
  <thead>
    <tr>
      <th>Pl</th>
      <th>Sig</th>
      <th>Name</th>
      <th>New records</th>
      <th>Override records</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @plugin_record_groups.each do |plugin_record_group| %>
      <tr>
        <td><%= plugin_record_group.pl_id %></td>
        <td><%= plugin_record_group.sig %></td>
        <td><%= plugin_record_group.name %></td>
        <td><%= plugin_record_group.new_records %></td>
        <td><%= plugin_record_group.override_records %></td>
        <td><%= link_to 'Show', plugin_record_group %></td>
        <td><%= link_to 'Edit', edit_plugin_record_group_path(plugin_record_group) %></td>
        <td><%= link_to 'Destroy', plugin_record_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Plugin record group', new_plugin_record_group_path %>
