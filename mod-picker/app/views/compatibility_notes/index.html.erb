<p id="notice"><%= notice %></p>

<h1>Listing Compatibility Notes</h1>

<table>
  <thead>
    <tr>
      <th>Cn</th>
      <th>Submitted by</th>
      <th>Mod mode</th>
      <th>Compatibility patch</th>
      <th>Compatibility status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @compatibility_notes.each do |compatibility_note| %>
      <tr>
        <td><%= compatibility_note.cn_id %></td>
        <td><%= compatibility_note.submitted_by %></td>
        <td><%= compatibility_note.mod_mode %></td>
        <td><%= compatibility_note.compatibility_patch %></td>
        <td><%= compatibility_note.compatibility_status %></td>
        <td><%= link_to 'Show', compatibility_note %></td>
        <td><%= link_to 'Edit', edit_compatibility_note_path(compatibility_note) %></td>
        <td><%= link_to 'Destroy', compatibility_note, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Compatibility note', new_compatibility_note_path %>
