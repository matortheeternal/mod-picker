<div class="scaffold">
  <p id="notice"><%= notice %></p>

  <h1>mod_stars</h1>

  <table>
    <thead>
      <tr>
          <th>mod_id</th>
          <th>user_id</th>
          <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @mod_stars.each do |mod_star| %>
        <tr>
            <td><%= mod_star.mod_id %></td>
            <td><%= mod_star.user_id %></td>
            <td><%= link_to 'Show', mod_star %></td>
          <td><%= link_to 'Edit', edit_mod_star_path(mod_star) %></td>
          <td><%= link_to 'Destroy', mod_star, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to 'New User Mod Star Map', new_mod_star_path %>
</div>