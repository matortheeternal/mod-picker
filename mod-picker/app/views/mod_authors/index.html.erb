<div class="scaffold">
  <p id="notice"><%= notice %></p>

  <h1>user_mod_author_maps</h1>

  <table>
    <thead>
      <tr>
          <th>mod_id</th>
          <th>user_id</th>
          <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @user_mod_author_maps.each do |user_mod_author_map| %>
        <tr>
            <td><%= user_mod_author_map.mod_id %></td>
            <td><%= user_mod_author_map.user_id %></td>
            <td><%= link_to 'Show', user_mod_author_map %></td>
          <td><%= link_to 'Edit', edit_user_mod_author_map_path(user_mod_author_map) %></td>
          <td><%= link_to 'Destroy', user_mod_author_map, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to 'New User Mod Author Map', new_user_mod_author_map_path %>
</div>