<p id="notice"><%= notice %></p>

<h1>Listing Helpful Marks</h1>

<table>
  <thead>
    <tr>
      <th>R</th>
      <th>Cn</th>
      <th>In</th>
      <th>Submitted by</th>
      <th>Helpful</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @helpful_marks.each do |helpful_mark| %>
      <tr>
        <td><%= helpful_mark.r_id %></td>
        <td><%= helpful_mark.cn_id %></td>
        <td><%= helpful_mark.in_id %></td>
        <td><%= helpful_mark.submitted_by %></td>
        <td><%= helpful_mark.helpful %></td>
        <td><%= link_to 'Show', helpful_mark %></td>
        <td><%= link_to 'Edit', edit_helpful_mark_path(helpful_mark) %></td>
        <td><%= link_to 'Destroy', helpful_mark, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Helpful mark', new_helpful_mark_path %>
